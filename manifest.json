{
    "name": "PriceFeed",
    "version": "0.01",
    "description": "Crypto PriceFeed for the Traders",
    "manifest_version":3,
    "action":{
        "default_icon": {        
            "24":"icon_eth.png",        
            "32":"icon_eth.png",        
            "16":"icon_eth.png"        
        },
        "default_popup":"popup.html",
        "default_title":"PriceFeed"
    },
    "icons":{
        "128":"icon_eth.png"
    },
    "content-scripts":[{
        "css": "style.css",
        "js": ["change.js"],
        "matches": ["<all_urls>"]
    }]
}